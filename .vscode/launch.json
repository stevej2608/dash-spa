{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "1. usage.py",
      "type": "python",
      "request": "launch",
      "python": "${command:python.interpreterPath}",
      "program": "${workspaceFolder}/usage.py",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": false,
      "serverReadyAction": {
        "pattern": ".*Visit http://(.*)",
        "uriFormat": "http://%s",
        "action": "openExternally"
      }
    },
    {
      "name": "2. examples/table.py",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "python": "${command:python.interpreterPath}",
      "module": "examples.table",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": false
    },





    
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "2. test.py",
      "type": "python",
      "request": "launch",
      "python": "${command:python.interpreterPath}",
      "program": "${workspaceFolder}/test.py",
      "args": [
        "runserver"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "3. examples/redirect.py",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "python": "${command:python.interpreterPath}",
      "module": "examples.redirect",
      "env": {},
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "4. PyTest",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "python": "${command:python.interpreterPath}",
      "module": "pytest",
      "args": [
        "--headless"
      ],
      "cwd": "${workspaceRoot}",
      "env": {},
      "envFile": "${workspaceRoot}/.env",
      "console": "internalConsole",
      "justMyCode": false
    }
  ]
}