    {
      "name": "1a. usage.py",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "usage.py",
      "console": "internalConsole",
      "env": {"debugpyHASHSEED": "1234"},
      "internalConsoleOptions": "openOnSessionStart",
      "justMyCode": false,
      "serverReadyAction": {
        "pattern": "Visit\\s+(https?://\\S+)",
        "uriFormat": "%s",
        "action": "debugWithChrome",
        "killOnServerStop": true
      }
    },

